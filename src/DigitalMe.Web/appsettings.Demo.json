{
  "DetailedErrors": false,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.SignalR": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=digitalme-demo.db"
  },
  "DigitalMe": {
    "ApiBaseUrl": "http://localhost:5003",
    "SignalRHub": "/chathub",
    "Authentication": {
      "JwtSecret": "demo-secret-key-for-presentation-32ch",
      "Issuer": "DigitalMe",
      "Audience": "DigitalMe-Demo"
    },
    "Features": {
      "UseRealSignalR": true,
      "UseRealAuthentication": false,
      "EnableLogging": true,
      "DemoMode": true,
      "PreloadedResponses": true
    },
    "Demo": {
      "MaxResponseTime": 2000,
      "EnableMetrics": true,
      "ShowSystemHealth": true,
      "OptimizeForDemo": true,
      "BackupMode": true
    },
    "Integrations": {
      "Slack": {
        "DemoToken": "demo-slack-token",
        "DemoWorkspace": "EllyAnalytics Demo",
        "MockResponses": true
      },
      "ClickUp": {
        "DemoToken": "demo-clickup-token", 
        "DemoSpace": "R&D Projects",
        "MockResponses": true
      },
      "GitHub": {
        "DemoToken": "demo-github-token",
        "DemoRepo": "EllyAnalytics/DigitalMe",
        "MockResponses": true
      },
      "Telegram": {
        "DemoToken": "demo-telegram-token",
        "DemoChatId": "demo-chat",
        "MockResponses": true
      }
    },
    "AI": {
      "Claude": {
        "DemoMode": true,
        "MaxTokens": 4000,
        "Temperature": 0.7,
        "PreloadedPersonality": true
      }
    },
    "Performance": {
      "EnableCaching": true,
      "CacheTimeout": 300,
      "EnableCompression": true,
      "OptimizeAssets": true
    }
  }
}